<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet>
        <Constraint
            android:id="@+id/textranda"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/button"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:transitionEasing="cubic(0,0.65,0.8,0.29)" />
        <Constraint
            android:id="@+id/button"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="183dp"
            motion:layout_constraintBottom_toTopOf="@+id/textViewranda"
            motion:layout_constraintTop_toBottomOf="@+id/textranda"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="232dp"
            motion:transitionEasing="cubic(0.83,0.29,0.8,0.8)" />
        <Constraint
            android:id="@+id/textViewranda"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button"
            motion:layout_constraintStart_toStartOf="parent"
            motion:transitionEasing="cubic(0.49,0.76,0.8,0.8)" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="467dp"
            motion:layout_editor_absoluteX="176dp"
            android:id="@+id/textView"
            motion:layout_constraintTop_toBottomOf="@+id/button"
            android:layout_marginTop="50dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="177dp"
            android:layout_marginRight="177dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="176dp"
            android:layout_marginLeft="176dp"
            motion:layout_constraintBottom_toTopOf="@+id/textViewranda"
            android:layout_marginBottom="159dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/textranda"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/button"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:transitionEasing="cubic(0.21,0.67,0.8,0.8)" />
        <Constraint
            android:id="@+id/button"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="183dp"
            motion:layout_constraintBottom_toTopOf="@+id/textViewranda"
            motion:layout_constraintTop_toBottomOf="@+id/textranda"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="232dp"
            motion:transitionEasing="cubic(0.2,0.2,0.18,0.85)" />
        <Constraint
            android:id="@+id/textViewranda"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/button"
            motion:layout_constraintStart_toStartOf="parent"
            motion:transitionEasing="cubic(0.2,0.2,1,0)" />
    </ConstraintSet>
</MotionScene>